  "scripts": {
    "prebuild": "npm run build:clean",
    "postinstall": "env-cmd ./.env if-env-defined FIEF_DATA_DB_URL && npm run migrate || npm run warn-no-db",
    "warn-no-db": "echo 'Set the FIEF_DATA_DB_URL environment variable and run `npm migrate` to set up the database' >&2",
    "build": "cross-env NODE_ENV=production webpack --env.NODE_ENV=production --config webpack.config.js --color --progress --hide-modules",
    "build:clean": "rimraf ./dist",
    "start": "per-env",
    "start:development": "cross-env NODE_ENV=development nodemon server/bin/www --debug",
    "start:production": "cross-env NODE_ENV=production node server/bin/www",
    "test": "cross-env NODE_ENV=test jest --watch",
    "test:prod": "cross-env NODE_ENV=test jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "pre-commit": "lint:staged",
